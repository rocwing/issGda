<?xml version="1.0" encoding="GBK"?>
<rdf:RDF xmlns:cim="http://iec.ch/TC57/2003/CIM-schema-cim10#" xmlns:rdf="http://www.w3.org/2000/01/rdf-schema#">
<cim:ConductingEquipment rdf:ID="PMS-ConductingEquipment@PMSID#/ConductingEquipment">
<cim:PowerSystemResource.Asset rdf:resource="#PMS-Asset@PMSID#/ConductingEquipment"/>
<!--cimadd-->
<cim:PowerSystemResource.coEquipmentNumber>ZHDQMC#</cim:PowerSystemResource.coEquipmentNumber><!--组合式设备编号-->
<cim:PowerSystemResource.ifCoEquipment>ZHSBLX#</cim:PowerSystemResource.ifCoEquipment><!--是否组合式设备组件-->
<cim:PowerSystemResource.remarks>BZ#</cim:PowerSystemResource.remarks><!--备注-->
<cim:PowerSystemResource.locCode>GNWZ#</cim:PowerSystemResource.locCode><!--功能位置-->
<cim:PowerSystemResource.ParentPSR rdf:resource="#PMS-EquipmentLocation@SCSBH#/EquipmentLocation"/><!--关联上层设备号-->
<cim:PowerSystemResource.dispatchNo>YXBH#</cim:PowerSystemResource.dispatchNo><!--运行编号-->
<cim:PowerSystemResource.Documents rdf:resource="#PMS-ElectricalAssetModel@PMSID#/ConductingEquipment"/><!--关联相数-->
<cim:Naming.name>SBMC#</cim:Naming.name><!--设备名称-->
<cim:PowerSystemResource.PSRType rdf:resource="#PMS-PSRType@CIMNODE#"/><!--关联设备类型-->
<cim:PowerSystemResource.unitName>JGDY#</cim:PowerSystemResource.unitName><!--间隔单元-->
<cim:Equipment.MemberOf_EquipmentContainer rdf:resource="#PMS-Substation@SUBPMSID#/Substation"/><!--关联变电站名称-->
<cim:Equipment.MemberOf_EquipmentContainer rdf:resource="#PMS-Storage@CKPMSID#/Storage"/><!--关联仓库名称-->
</cim:ConductingEquipment>

<cim:ElectricalAssetModel  rdf:ID="PMS-ElectricalAssetModel@PMSID#/ConductingEquipment">
<cim:Naming.name>SBXH#</cim:Naming.name><!--设备型号-->
<cim:ElectricalAssetModel.ratedVolts>EDDY#</cim:ElectricalAssetModel.ratedVolts><!--额定电压-->
<cim:ElectricalAssetModel.numberPhases>SXHDX#</cim:ElectricalAssetModel.numberPhases><!--相数-->
<cim:ElectricalAssetModel.leakageDistance >PDBJ#</cim:ElectricalAssetModel.leakageDistance ><!--泄露比距-->
</cim:ElectricalAssetModel>

<cim:Asset rdf:ID="PMS-Asset@PMSID#/ConductingEquipment">
<!--cimassetadd-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-CZCJDLXCY/ConductingEquipment"/><!--关联操作冲击电流下残压-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-DPCJDLXCY/ConductingEquipment"/><!--关联陡波冲击电流下残压-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-FBTLRL/ConductingEquipment"/><!--关联方波通流容量-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-MXJS/ConductingEquipment"/><!--关联每相节数-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-LDCJCY_FZ/ConductingEquipment"/><!--关联标准雷电残压-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-BCFDDL/ConductingEquipment"/><!--关联标称放电电流-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-SFDJX/ConductingEquipment"/><!--关联是否带间隙-->
<cim:Asset.Properties rdf:resource="#PMS-Property@PMSID#-JYTLX/ConductingEquipment"/><!--关联绝缘体类型-->
<cim:Asset.assetType>LX#</cim:Asset.assetType><!--类型-->
<cim:Asset.serialNumber>GDZCBM#</cim:Asset.serialNumber><!--固定资产编码-->
<cim:Asset.currentStatus>YXZT#</cim:Asset.currentStatus><!--设备状态-->
<cim:Asset.antipollutionGrade>FWDJ#</cim:Asset.antipollutionGrade><!--防污等级-->
<cim:Asset.installationEnviron>SYHJ#</cim:Asset.installationEnviron><!--使用环境-->
<cim:Asset.plantTransferDate>TYRQ#</cim:Asset.plantTransferDate><!--投运日期-->
<cim:Asset.manufacturedDate>CCRQ#</cim:Asset.manufacturedDate><!--出厂日期-->
<cim:Asset.codeName>CPDH#</cim:Asset.codeName><!--产品代号-->
<cim:Asset.utc>CCBH#</cim:Asset.utc><!--出厂编号-->
<cim:Asset.factory>SCCJ#</cim:Asset.factory><!--生产厂家-->
<cim:Asset.manufacturedCountry>ZZGJ#</cim:Asset.manufacturedCountry><!--制造国家-->
<cim:Asset.voltGrade>DYDJ#</cim:Asset.voltGrade><!--电压等级-->
<cim:Asset.phases>XB#</cim:Asset.phases><!--相别-->
<cim:Asset.Organisations rdf:resource="#PMS-Organisation@assetOrganisation#"/><!--资产归属二级单位-->
<cim:Asset.projectNo>GCBH#</cim:Asset.projectNo>
</cim:Asset>
<!--cimassertoutsideadd-->
<!--cimproperty-->
<cim:Property rdf:ID="PMS-Property@PMSID#-FBTLRL/ConductingEquipment"><!--方波通流容量-->
<cim:Naming.name>方波通流容量</cim:Naming.name>
<cim:Property.propertyType>FBTLRL</cim:Property.propertyType>
<cim:Property.propertyValue>FBTLRL#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-MXJS/ConductingEquipment"><!--每相节数-->
<cim:Naming.name>每相节数</cim:Naming.name>
<cim:Property.propertyType>MXJS</cim:Property.propertyType>
<cim:Property.propertyValue>MXJS#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-LDCJCY_FZ/ConductingEquipment"><!--标准雷电残压-->
<cim:Naming.name>标准雷电残压</cim:Naming.name>
<cim:Property.propertyType>LDCJCY_FZ</cim:Property.propertyType>
<cim:Property.propertyValue>LDCJCY_FZ#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-BCFDDL/ConductingEquipment"><!--标称放电电流-->
<cim:Naming.name>标称放电电流</cim:Naming.name>
<cim:Property.propertyType>BCFDDL</cim:Property.propertyType>
<cim:Property.propertyValue>BCFDDL#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-SFDJX/ConductingEquipment"><!--是否带间隙-->
<cim:Naming.name>是否带间隙</cim:Naming.name>
<cim:Property.propertyType>SFDJX</cim:Property.propertyType>
<cim:Property.propertyValue>SFDJX#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-JYTLX/ConductingEquipment"><!--绝缘体类型-->
<cim:Naming.name>绝缘体类型</cim:Naming.name>
<cim:Property.propertyType>JYTLX</cim:Property.propertyType>
<cim:Property.propertyValue>JYTLX#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-CZCJDLXCY/ConductingEquipment"><!--操作冲击电流下残压-->
<cim:Naming.name>操作冲击电流下残压</cim:Naming.name>
<cim:Property.propertyType>CZCJDLXCY</cim:Property.propertyType>
<cim:Property.propertyValue>CZCJDLXCY#</cim:Property.propertyValue>
</cim:Property>
<cim:Property rdf:ID="PMS-Property@PMSID#-DPCJDLXCY/ConductingEquipment"><!--陡波冲击电流下残压-->
<cim:Naming.name>陡波冲击电流下残压</cim:Naming.name>
<cim:Property.propertyType>DPCJDLXCY</cim:Property.propertyType>
<cim:Property.propertyValue>DPCJDLXCY#</cim:Property.propertyValue>
</cim:Property>
</rdf:RDF>
