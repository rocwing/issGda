<?xml version="1.0" encoding="GBK"?>
<rdf:RDF xmlns:cim="http://iec.ch/TC57/2003/CIM-schema-cim10#" xmlns:rdf="http://www.w3.org/2000/01/rdf-schema#">
<cim:Bay rdf:ID="PMS-Bay@PMSID#/Bay">
<cim:PowerSystemResource.Asset rdf:resource="#PMS-Asset@PMSID#/Bay"/>
<!--cimadd-->
<cim:PowerSystemResource.locCode>GNWZ#</cim:PowerSystemResource.locCode><!--功能位置-->
<cim:PowerSystemResource.PSRType rdf:resource="#PMS-PSRType@CIMNODE#"/><!--关联间隔单元的设备类型-->
<cim:PowerSystemResource.dispatchNo>JGBH#</cim:PowerSystemResource.dispatchNo><!--单元编号-->
<cim:Naming.name>JGMC#</cim:Naming.name><!--单元名称-->
